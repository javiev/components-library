{
	"testEnvironment": "jsdom",
	"moduleNameMapper": {
		"\\.(css|less|scss|sass)$": "identity-obj-proxy"
	},
	"setupFilesAfterEnv": ["<rootDir>/setupTest.ts"],
	"coverageThreshold": {
		"global": {
			"branches": 80,
			"functions": 80,
			"lines": 80,
			"statements": 80
		}
	},
	"testMatch": ["<rootDir>/src/**/?(*.)+(test).tsx"]
}
